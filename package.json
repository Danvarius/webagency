  "description": "Личный проект Web-Agency",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.2.x",
    "browser-sync": "^2.26.14",
    "critical": "^2.0.0-27",
    "del": "^6.0.0",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.x",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-deploy-git": "^0.5.3",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "9.0.x",
    "gulp-rename": "^2.0.x",
    "gulp-sass": "4.1.x",
    "gulp-sourcemaps": "3.0.x",
    "gulp-uglify-es": "^2.0.0",
    "gulp-webp": "^4.0.1",
    "gulp-webp-html": "^1.0.2",
    "imagemin-webp": "^5.1.x",
    "path": "^0.12.7",
    "stylelint": "13.11.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Danvarius/webagency"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax sass",
